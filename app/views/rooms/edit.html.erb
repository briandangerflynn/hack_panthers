<nav>
  <div class="row" id="navbar">
  <div class="nav-wrapper">
        <h2 class="col s2 white-text" id="navlogo"><a href='/'><%= image_tag("homeologo.svg", class:"homeo")%>   Homeo</a></h2>
      <ul id="nav-mobile" class="right hide-on-med-and-down">
        <% if current_owner %>
          <li><a href="/" class="navlink">Home</a></li>
          <li><a href="/owners/<%= current_owner.id %>">Signed in as <%= current_owner.email %></a></li>
          <li><%= link_to "Logout", '/renterlogout', :class=>"navlink" %></li>
        <% else %>
          <li><a href="/" class="navlink">Home</a></li>
          <li><%= link_to 'Login', '/ownerlogin' , :class=>"navlink" %></li>
          <li><%= link_to 'Signup', '/owners/signup', :class=>"navlink" %></li>
        <% end %>
      </ul>
  </div>
  </div>
</nav>
<div class="container">
<%= form_for @room, url: {action: 'update'} do |f| %>

  <%= f.label :title %>
  <br>
  <%= f.text_field :title, value: @room.title %>
  <br>
  <%= f.label :address %>
  <br>
  <%= f.text_field :address, value: @room.address %>
  <br>
  <%= f.label :borough %>
  <br>
  <%= f.text_field :borough, value: @room.borough %>
  <br>
  <%= f.label :neighborhood %>
  <br>
  <%= f.text_field :neighborhood, value: @room.neighborhood  %>
  <br>
  <%= f.label :description %>
  <br>
  <%= f.text_field :description, value: @room.description %>
  <br>
  <%= f.label :price %>
  <br>
  <%= f.text_field :price, value: @room.price %>
  <br>
  <%= f.check_box :pets_allowed, {}, "true", "false" %>
  <%= f.label :pets_allowed %>
  <br>
  <%= f.check_box :available, {}, "true", "false" %>
  <%= f.label :available %>
  <br>

  <%= f.label :video_url %>
  <%= f.text_field :video_url, value: @room.video_url %>
  <br>

 <%= f.submit %>
 <br>
<% end %>
  <button> <%= link_to 'Cancel', '/rooms' %></button>
  <br><br>
<%= link_to 'Delete', @room, method: :delete %>
<br>

</div>
