
<nav>
  <div class="row" id="navbar">
  <div class="nav-wrapper">
        <h2 class="col s2 white-text" id="navlogo"><a href='/'><%= image_tag("homeologo.svg", class:"homeo")%>   Homeo</a></h2>
      <ul id="nav-mobile" class="right hide-on-med-and-down">
        <% if current_renter %>
          <li><a href="/" class="navlink">Home</a></li>
          <li><a href="/renters/<%= current_renter.id %>">Signed in as <%= current_renter.email %></a></li>
          <li><%= link_to "Logout", '/renterlogout', :class=>"navlink" %></li>
        <% else %>
          <li><a href="/" class="navlink">Home</a></li>
          <li><%= link_to 'Login', '/renterlogin' , :class=>"navlink" %></li>
          <li><%= link_to 'Signup', '/renters/signup', :class=>"navlink" %></li>
        <% end %>
      </ul>
  </div>
  </div>
</nav>
<div class="container">
<h1>Sign Up to Get Started</h1>

  <%= form_for :renter, url: {action: "create"} do |f| %>
    <%= label(:renter, :name) %>
    <%= f.text_field :name, :autofocus => "autofocus"  %>
      <br><br>
    <%= label(:renter, :email) %>
    <%= f.text_field :email %>
      <br><br>
    <%= label(:renter, :password) %>
    <%= f.password_field :password %>
      <br><br>
    <%= f.submit "Add Renter" %>
  <% end %>
</div>
