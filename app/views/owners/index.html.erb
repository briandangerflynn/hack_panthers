<div class="container">

<nav class="nav">
    <% if current_owner %>
       Signed in as <%= current_owner.email %> |
       <%= link_to "Logout", '/ownerlogout', :class=>"navlink" %> |
    <% else %>
       <%= link_to 'Login', '/ownerlogin' %>
       <%= link_to 'Signup', '/owners/signup', :class=>"navlink" %>
    <% end %>
     <a href="/" class="navlink">Home</a>

</nav>
<br><br>

<h1>Owners!</h1>
  <% @owners.each do |owner| %>
    <h2>Name: <%= link_to owner.name, owner %></h2>


  <% end %>

  <br /><br />
  <h3><%= link_to 'Sign up! Be an owner!', new_owner_path %></h3>
</div>
